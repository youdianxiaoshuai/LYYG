.my-reserve-item{
	display: flex;
	padding: 30rpx;
	border-bottom: 1rpx solid #ddd;
}
.my-reserve-img{
	width: 150rpx;
	height: 150rpx;
}
.my-reserve-img image{
	width: 100%;
	height: 100%;
}

.my-reserve-text{
	flex: 1;
	margin-left: 30rpx;
	width: 100%;
}
.my-reserve-title{
	color: #333;
	width: 100%;
	font-size: 34rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.my-reserve-text text{
	display: block;
	color: #888;
  font-size: 20rpx;
}
.my-reserve-nocomment text{
  color:#FF0000;
}

.my-reserve-comment text{
  color:#00FF00;
}

.my-reserve-prices text{
  text-align:right;
  color:#333;
  font-size: 25rpx
}
